{
  "manifest_version": 2,
  "name": "OpenLinks",
  "version": "1.0",

  "description": "Opens links.",
  "homepage_url": "https://jimmyaffatigato.github.io",
  "icons": {
    "48": "icons/icon-48.png"
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
